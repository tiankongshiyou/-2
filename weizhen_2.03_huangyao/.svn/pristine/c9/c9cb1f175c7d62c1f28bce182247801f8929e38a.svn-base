/* pages/center/center_cup/center_cup.wxss */

page {
  height: 100%;
  font-size: 30rpx;
}

.swiper-tab {
  width: 100%;
  text-align: center;
  height: 100rpx;
  line-height: 100rpx;
  background-color: #fff;
  display: flex;
  flex-flow: row;
  justify-content: space-around;
  border-top: 1rpx solid #f5f5f5;
}

.swiper-tab-item {
  width: 12%;
  color: #333;
}

.active {
  color: #ff5d00;
  border-bottom: 4rpx solid #ff5d00;
}

swiper {
  height: 100%;
  text-align: center;
}
.bintap{
  display: inline;
}
.onedata {
  width: 100%;
  display: flex;
  flex-direction: column;
  margin-top:16rpx;
}

.cup_user {
  width: 100%;
  /* background: salmon; */
  display: flex;
  justify-content: center;
  align-items: center;
  margin-bottom: 10rpx;
}

.cup_userimg {
  width: 732rpx;
  height: 289rpx;
  margin-left:10rpx;
  position: relative;
}

.cupimg {
  width: 732rpx;
  height: 289rpx;
  position: absolute;
  left: 0;
  top: 0;
}

.cup_usertxt {
  width: 685rpx;
  height: 260rpx;
  position: absolute;
  top: 16rpx;
  right: 20rpx;
}
.cup_name text, .cup_data text, .cup_moneystop text, .cup_man text{
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.cup_name {
  width: 100%;
  height:60rpx;
  align-items: center;
  font-size: 30rpx;
  color: #4d4d4d;
  display: flex;
  justify-content: flex-start;
}
.cup_man{
  height:70rpx;
  display: flex;
  align-items: flex-end;
}
.cup_data {
  width: 65%;
  height:50rpx;
  display: flex;
  align-items: center;
  justify-content: flex-start;
  font-size: 26rpx;
  color: #808080;
 
}

.cup_moneys {
  width: 100%;
  height: 120rpx;
  display: flex;
  margin-top:20rpx;
  justify-content: center;
  align-items: center;
}

.cup_moneystop {
  font-size: 40rpx;
  display: flex;
  align-items: center;
  color: #4d4d4d;
  margin-right: 24rpx;
}
.cup_toumimg, .cup_users{
 width: 732rpx;
  height: 289rpx;
  position: absolute;
  top:0;
  left:0;
}
.cup_users{
  display: flex;
  align-items: center;
  justify-content: center;
  z-index: 999;
}
.cup_users image{
  width:199rpx;
  height:154rpx;
}
.unusedNumber{
  width: 100%;
  height:800rpx;
  color: #4d4d4d;
  display: flex;
  justify-content: center;
  align-items: center;
}
