}

.coupon_xze, .shop_xze, .fen_xze {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.3);
}

.hiedshow {
  width: 100%;
  height: 100%;
