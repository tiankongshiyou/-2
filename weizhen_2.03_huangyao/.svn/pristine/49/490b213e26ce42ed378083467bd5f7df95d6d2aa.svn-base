/* pages/center/withdraw_cash/withdraw_cash.wxss */

page {
  color: #4d4d4d;
}

.withdraw_cash {
  width: 100%;
  display: flex;
  flex-direction: column;
}

.withdraw_top {
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}

.withdraw_img {
  width: 93.5%;
  height: 302rpx;
  display: flex;
  justify-content: center;
  margin: 16rpx 0 16rpx 0;
  position: relative;
  background: #fff;
}

.withdraw_img image {
  width: 100%;
  height: 100%;
}

.withdraw_view {
  width: 90%;
  height: 290rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  color: #4d4d4d;
  font-size: 34rpx;
}

.withdraw_lei {
  width: 100%;
  margin: 20rpx 0 10rpx 0;
  display: flex;
  color: #ff5d00;
  justify-content: space-between;
  align-items: flex-end;
}

.with_text {
  width: 100%;
  height: 60rpx;
  display: flex;
  justify-content: space-between;
  align-items: flex-end;
  position: relative;
}
.imgpostion{
  width:36rpx;
  height: 60rpx;
  position: absolute;
  right:0;
  bottom:0;
  display: flex;
  align-items: flex-end;
}
.with_text text {
  width: 90%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-size: 30rpx;
}

.with_text image {
  width: 36rpx;
  height: 36rpx;
}

.withdraw_money {
  width: 100%;
  display: flex;
  justify-content: center;
  background: #fff;
}

.withdraw_cont {
  width: 86.5%;
  display: flex;
  flex-direction: column;
}

 .withdraw_account {
  width: 100%;
  height:100rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  
}
.withdraw_zh{
   display: flex;
  justify-content: flex-start;
  align-items: center;
font-size: 26rpx;
  color: #4d4d4d;
}
.withdraw_account, .withdraw_quota {
  border-bottom: 1rpx solid #e6e6e6;
}

.withdraw_zhbtn {
  display: flex;
  justify-content: flex-start;
  align-items: center;
}

.withdraw_zhbtn image {
  width: 34rpx;
  height: 34rpx;
  margin-left: 10rpx;
}

.withdraw_quota {
  width: 100%;
  height: 164rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  font-size: 34rpx;
}

.withdraw_input {
  width: 100%;
  display: flex;
  justify-content: flex-start;
  align-items: center;
}

.btn {
  width: 90%;
  height: 88rpx;
  background: #ff5d00;
  color: #fff;
  border-radius: 16rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  margin: 180rpx auto 0;
  font-size: 32rpx;
}

.sssss {
  width: 50rpx;
  height: 50rpx;
  border: 1rpx solid #4d4d4d;
  position: relative;
}

.aaaa {
  position: absolute;
  top: 190rpx;
  left: 10rpx;
}
