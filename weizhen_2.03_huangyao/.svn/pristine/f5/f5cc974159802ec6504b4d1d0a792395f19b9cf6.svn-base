page {
  background: #f3f5f7;
}

.auditing_finally {
  height: 554rpx;
  margin-top: 16rpx;
  background: #fff;
  display: flex;
  align-items: center;
  flex-direction: column;
}

.auditing_state {
  display: flex;
  justify-content: center;
}

.auditing_state image {
  width: 160rpx;
  height: 160rpx;
  padding-top: 30rpx;
}

.state {
  color: #4d4d4d;
  font-size: 32rpx;
  margin-top: 34rpx;
  text-align: center;
}

.auditing_datum {
  margin-top: 42rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.auditing_datum text {
  color: #ff5d00;
  font-size: 30rpx;
}

.auditing_datum image {
  width: 16rpx;
  height: 27rpx;
  margin-left: 16rpx;
}

.state_reason {
  height: 110rpx;
  line-height: 38rpx;
  color: #b3b3b3;
  font-size: 26rpx;
  text-indent: 60rpx;
  margin: 62rpx 83rpx 0 83rpx;
}