/* pages/center/my_team/my_team.wxss */

/* pages/center/center_cup/center_cup.wxss */

page {
  height: 100%;
  font-size: 30rpx;
}

.swiper-tab {
  width: 100%;
  text-align: center;
  height: 100rpx;
  line-height: 100rpx;
  background-color: #fff;
  display: flex;
  flex-flow: row;
  justify-content: space-around;
  border-top: 1rpx solid #f5f5f5;
  box-shadow: 0 2rpx 10rpx #ccc;
}

.swiper-tab-item {
  width: 12%;
  color: #333;
}

.active {
  color: #ff5d00;
  border-bottom: 4rpx solid #ff5d00;
}

swiper {
  height: 100%;
  text-align: center;
}

.my_shophome, .my_getetshop {
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-top: 16rpx;
  background: #fff;
  z-index: 999;
}

.my_shopuser {
  width: 92%;
  margin: 0 auto 0;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}

.my_user {
  width: 100%;
  height:120rpx;
  display: flex;
  justify-content:flex-start;
  align-items: center;
}

.my_user image {
  width: 74rpx;
  height: 74rpx;
  border-radius: 50%;
  background: skyblue;
  margin-right:16rpx;
}
.my_usertxt{
  width:80%;
  text-align: left;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.my_userbtn{
  width: 96%;
  height:60rpx;
  display: flex;
  justify-content:space-between;
  align-items:flex-start;
  margin:0 auto 0;
  color: #808080;
  border-bottom:1rpx solid #f5f5f5;
}
.my_userbtn text{
  width:50%;
  text-align: left;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

